//----Parameters Definition-----

def plan           = "west"   // already defined in the console

if(plans.get(plan)==null)
	println "Could not find initial plan "+plan
def initialPlan = new Plan(console,plans.get(plan))
def start = initialPlan.initialLocation()
def waypoints = initialPlan.waypoints()

new Plan(console).with{
	
	planName "east1"
	def gt = waypoints[1].translateBy(0,-4500)
	goTo location:gt.translateBy(0,-100)
	goTo location:gt
	def limit = waypoints[1]
     yoyo location: limit,max_depth:7.5,min_depth:2.5, pitch:15
     (1..3).each{
     popup location: limit,duration: 60.0,waitAtSurface:true
     }
     skeeping location: limit,duration:0.0
	addToConsole()
}
println "done"