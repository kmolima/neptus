//----Parameters Definition-----

def popDistance    = 500 //in meters
def plan           = "survey2"   // already defined in the console
double speed_value = 700.0
def speed_unit     = "RPM"
double z_value     = 0.0
def z_unit         = "DEPTH"
if(plans.get(plan)==null)
	println "Could not find initial plan "+plan
def initialPlan = new Plan(console,plans.get(plan))
def start = initialPlan.initialLocation()
def home = Location.APDL

new Plan(console).with{
	
	planName "survey2_start"
	
	speed speed_value,speed_unit
	z z_value,z_unit
	goTo location: midpoint(home,start,60)
	
	    vehicles "lauv-noptilus-1","lauv-noptilus-2","lauv-noptilus-3"
	    addToConsole()
}
println "done"