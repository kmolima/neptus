lat = location 41.185242
lon = location(ask 'Longitude?') //-8.70701

def loc = new Location(lat,lon)
message loc
uuv_camera = pick {

	payload 'Camera' //noptilus-3
}
task = imcPlan {
	
	planName 'Camera_Plan'
	locate Location.APDL
	goTo location: loc, payload: [[name: "Camera"]]
}
execute uuv_camera:task
