
v1 = pick { 
  type 'UUV'
  id 'lauv-seacon-1'
}

v2 = pick { 
  type 'UUV'
  id 'lauv-seacon-2'
}

v3 = pick {
  type 'UUV'
  id 'lauv-seacon-3'
}
execute v1: 
           imcPlan('test1') >> action { post x:1 },
        v2: 
           imcPlan('test2') >> action { post x:2 },
        v3:
           allOf {
              when { consume x:1 } then imcPlan('test3')
              when { consume x:2 } then imcPlan('test4')
           }
