// Pick an UUV ---- ~ 38.43461666666666 -8.861177777777778 : Comporta Op Area location

setConnectionTimeout  120.0

ComportaOpArea = location 38.425711111111106, -8.852072222222223
v = pick { 
  type 'UUV'
  region ComportaOpArea, 5.km
  id 'lauv-noptilus-1'
}
// IMC plan defined programatically
task = imcPlan {
    planName 'imc-dsl-plan'
    speed 1.5, Speed.Units.METERS_PS
    z     0.0, Z.Units.DEPTH
    locate new Location(Location.toRadians(38.425711111111106),Location.toRadians(-8.852072222222223))
    move 50,-125
    goTo()
    move 0,-50
    loiter()
}
 execute v:task